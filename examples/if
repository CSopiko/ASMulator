DEFINE <ifClause>;
SP = SP - 4;
M[SP] = 2;
SP = SP - 4;
M[SP] = 3;
R1 = SP + 4;
R1 = M[R1];
R2 = M[SP];
BLE R1, R2, PC + 36;
SP = SP - 4;
M[SP] = 3;
RV = M[SP];
SP = SP + 4;
SP = SP + 4;
SP = SP + 4;
RET;
R1 = SP + 4;
R1 = M[R1];
R2 = M[SP];
BGE R1, R2, PC + 36;
SP = SP - 4;
M[SP] = 4;
RV = M[SP];
SP = SP + 4;
SP = SP + 4;
SP = SP + 4;
RET;
SP = SP + 4;
SP = SP + 4;
RET;
END_DEF;
CALL <ifClause>;
PRINT <RV>;
